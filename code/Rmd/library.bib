file = {:home/ubuntu/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Ishida et al. - 2007 - Essential roles of the CC chemokine ligand 3-CC chemokine receptor 5 axis in bleomycin-induced pulmonary fibrosis.pdf:pdf},
isbn = {0002-9440 (Print){\$}\backslashbackslash{\{}\backslash{\$}{\}}r0002-9440 (Linking)},
issn = {00029440},
journal = {American Journal of Pathology},
number = {3},
pages = {843--854},
pmid = {17322370},
title = {{Essential roles of the CC chemokine ligand 3-CC chemokine receptor 5 axis in bleomycin-induced pulmonary fibrosis through regulation of macrophage and fibrocyte infiltration}},
