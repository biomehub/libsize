journal = {Mucosal Immunology},
number = {1},
pages = {240--253},
pmid = {26153764},
publisher = {Nature Publishing Group},
title = {{A protective role for IL-13 receptor {\$}\alpha{\$} 1 in bleomycin-induced pulmonary injury and repair}},
url = {http://dx.doi.org/10.1038/mi.2015.56},
volume = {9},
year = {2016}
}
