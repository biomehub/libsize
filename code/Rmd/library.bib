keywords = {Acute lung injury,Acute respiratory distress syndrome,Fat embolism,Hyperthermia,Oleic acid},
number = {6},
pages = {304--308},
pmid = {22819303},
publisher = {Elsevier Ireland Ltd},
title = {{Molecular-biological analysis of acute lung injury (ALI) induced by heat exposure and/or intravenous administration of oleic acid}},
url = {http://dx.doi.org/10.1016/j.legalmed.2012.06.003},
volume = {14},
