issn = {14654644},
journal = {Biostatistics},
keywords = {Batch effects,Empirical Bayes,Microarrays,Monte Carlo,batch effects,empirical bayes,microarrays,monte carlo},
number = {1},
pages = {118--127},
pmid = {16632515},
title = {{Adjusting batch effects in microarray expression data using empirical Bayes methods}},
url = {http://biostatistics.oxfordjournals.org/content/8/1/118.abstract{\%}5Cnhttp://biostatistics.oxfordjournals.org/content/8/1/118.full.pdf{\%}5Cnhttp://biostatistics.oxfordjournals.org/cgi/content/abstract/8/1/118},
volume = {8},
