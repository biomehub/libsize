file = {:home/ubuntu/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Yamaguchi et al. - 2017 - Protective effects of safflor yellow on acute lung injury induced by oleic acid in rats(12).pdf:pdf},
isbn = {1530-0293 (Electronic){\$}\backslashbackslash{\{}\backslash{\$}{\}}n0090-3493 (Linking)},
issn = {0090-3493},
journal = {Crit Care Med},
keywords = {*Disease Models,Adult/chemically,Animal,Animals,Hemodynamics/drug effects,Hypnotics and Sedatives/pharmacology/*therapeutic,Male,Oleic Acid/antagonists {\&} inhibitors/blood/toxicity,Propofol/pharmacology/*therapeutic use,Rats,Respiratory Distress Syndrome,Sprague-Dawley,induced/pathology/*prevention {\&} control},
