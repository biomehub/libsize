isbn = {1535-4970},
issn = {1073449X},
journal = {American Journal of Respiratory and Critical Care Medicine},
keywords = {Adhesion molecules,Biomarkers,MMP-7,Mortality prediction,Personalized medicine},
number = {1},
pages = {67--76},
pmid = {22016448},
title = {{Peripheral blood proteins predict mortality in idiopathic pulmonary fibrosis}},
volume = {185},
year = {2012}
}
