journal = {Journal of Thoracic Disease},
keywords = {Atelectasis,Consolidations,Guidelines,Interstitial syndrome,Malignancies,Pleural effusion (PE),Pneumonia,Pulmonary thromboembolism},
number = {6},
pages = {1356--1365},
pmid = {27293860},
title = {{Lung B-line artefacts and their use}},
volume = {8},
