number = {3},
pages = {684--693},
pmid = {24172847},
publisher = {Nature Publishing Group},
title = {{IL-1{\$}\backslashbackslashalpha{\{}\backslash{\{}{\}}\backslashbackslash{\{}\backslash{\$}{\}}{\{}$\backslash${\}}{\}}released from damaged epithelial cells is sufficient and essential to trigger inflammatory responses in human lung fibroblasts.}},
