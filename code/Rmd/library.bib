issn = {13624962},
journal = {Nucleic Acids Research},
number = {17},
pages = {9860--9873},
pmid = {28973476},
publisher = {Oxford University Press},
title = {{Meta-analysis of transcriptomic datasets identifies genes enriched in the mammalian circadian pacemaker}},
volume = {45},
year = {2017}
}
