file = {:home/ubuntu/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Huang et al. - 2017 - Microbes Are Associated with Host Innate Immune Response in Idiopathic Pulmonary Fibrosis.pdf:pdf},
issn = {1073-449X},
