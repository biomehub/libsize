keywords = {Dendritic cells,Fungal infection,Lung,Neutrophils,Phagocytosis,Viral infection},
number = {1},
pages = {189--214},
pmid = {23278750},
title = {{Cellular immune reactions in the lung}},
volume = {251},
year = {2013}
}
