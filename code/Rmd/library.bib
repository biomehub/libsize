doi = {10.1016/j.rmed.2015.06.003},
file = {:home/ubuntu/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Morais et al. - 2015 - Serum metalloproteinases 1 and 7 in the diagnosis of idiopathic pulmonary fibrosis and other interstitial pneumon.pdf:pdf},
isbn = {0954-6111 1532-3064},
issn = {15323064},
journal = {Respiratory Medicine},
