@article{Wells2003,
abstract = {In idiopathic pulmonary fibrosis, the quantitation of disease severity using pulmonary function tests is often confounded by emphysema. We have identified the composite physiologic index (CPI) most closely reflecting the morphologic extent of pulmonary fibrosis. Consecutive patients with a clinical/computed tomography (CT) diagnosis of idiopathic pulmonary fibrosis (n = 212) were divided into group I (n = 106) and group II (n = 106). The CPI was derived in group I (by fitting pulmonary function tests against disease extent on CT) and was tested in Group II. The formula for the CPI was as follows: extent of disease on CT = 91.0 - (0.65 x percent predicted diffusing capacity for carbon monoxide [DLCO]) - (0.53 x percent predicted FVC) + (0.34 x percent predicted FEV1). In group II, the CPI correlated more strongly with disease extent on CT (r2 = 0.51) than the individual pulmonary function test (DLCO the highest value, r2 = 0.38). A subanalysis demonstrated that the better fit of the CPI was ascribable to a correction of the confounding effects of emphysema. Mortality was predicted more accurately by the CPI than by a pulmonary function test in all clinical subgroups, including a separate cohort of 36 patients with histologically proven usual interstitial pneumonia (CPI, p {\textless}0.0005; FVC, p = 0.002; PO2, p = 0.002). In conclusion, a new CPI, derived against CT and validated using split sample testing, is a more accurate prognostic determinant in usual interstitial pneumonia than an individual pulmonary function test.},
author = {Wells, Athol U and Desai, Sujal R and Rubens, Michael B and Goh, Nicole S L and Cramer, Derek and Nicholson, Andrew G and Colby, Thomas V and {Du Bois}, Roland M and Hansell, David M},
doi = {10.1164/rccm.2111053},
file = {:home/ubuntu/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Wells et al. - 2003 - Idiopathic pulmonary fibrosis A composite physiologic index derived from disease extent observed by computed tomog.pdf:pdf},
isbn = {1073-449X (Print){\$}\backslashbackslash{\{}\backslash{\$}{\}}r1073-449X (Linking)},
issn = {1073449X},
journal = {American Journal of Respiratory and Critical Care Medicine},
keywords = {Composite physiologic index,Idiopathic pulmonary fibrosis,Prognosis,Pulmonary function tests},
number = {7},
pages = {962--969},
pmid = {12663338},
title = {{Idiopathic pulmonary fibrosis: A composite physiologic index derived from disease extent observed by computed tomography}},
volume = {167},
year = {2003}
}
@article{Raes2002,
abstract = {{\textless}p{\textgreater}Although it is well-established that macrophages can occur in distinct activation states, the molecular characteristics of differentially activated macrophages, and particularly those of alternatively activated macrophages (aaM{\$}\backslashbackslashphi{\{}\backslash{\{}{\}}\backslashbackslash{\{}\backslash{\$}{\}}{\{}$\backslash${\}}{\}}), are still poorly unraveled. Recently, we demonstrated that the expression of FIZZ1 and Ym is induced in aaM{\$}\backslashbackslashphi{\{}\backslash{\{}{\}}\backslashbackslash{\{}\backslash{\$}{\}}{\{}$\backslash${\}}{\}}as compared with classically activated macrophages (caM{\$}\backslashbackslashphi{\{}\backslash{\{}{\}}\backslashbackslash{\{}\backslash{\$}{\}}{\{}$\backslash${\}}{\}}), elicited {\textless}italic{\textgreater}in vitro{\textless}/italic{\textgreater}or developed {\textless}italic{\textgreater}in vivo{\textless}/italic{\textgreater}during infection with {\textless}italic{\textgreater}Trypanosoma brucei brucei{\textless}/italic{\textgreater}. In the present study, we analyzed the expression of FIZZ1 and Ym in caM{\$}\backslashbackslashphi{\{}\backslash{\{}{\}}\backslashbackslash{\{}\backslash{\$}{\}}{\{}$\backslash${\}}{\}}and aaM{\$}\backslashbackslashphi{\{}\backslash{\{}{\}}\backslashbackslash{\{}\backslash{\$}{\}}{\{}$\backslash${\}}{\}}elicited during {\textless}italic{\textgreater}Trypanosoma congolense{\textless}/italic{\textgreater}infection and show that the use of FIZZ1 and Ym for the identification of aaM{\$}\backslashbackslashphi{\{}\backslash{\{}{\}}\backslashbackslash{\{}\backslash{\$}{\}}{\{}$\backslash${\}}{\}}is not limited to {\textless}italic{\textgreater}T. b. brucei{\textless}/italic{\textgreater}infection and is independent of the organ sources from which macrophages are obtained. We also demonstrate that FIZZ1 can be used to discriminate between different populations of aaM{\$}\backslashbackslashphi{\{}\backslash{\{}{\}}\backslashbackslash{\{}\backslash{\$}{\}}{\{}$\backslash${\}}{\}}. Furthermore, we studied the effects of various stimuli, and combinations thereof, on the expression of FIZZ1 and Ym in macrophages from different mouse strains and demonstrate that regulation of the expression of FIZZ1 and Ym in macrophages is not dependent on the mouse strain. Finally, we show that these genes can be used to monitor the macrophage activation status without the need to obtain pure macrophage populations. {\textless}/p{\textgreater}},
author = {Raes, Geert and No{\"{e}}l, Wim and Beschin, Alain and Brys, Lea and de Baetselier, Patrick and Hassanzadeh, Gh. Gholamreza},
