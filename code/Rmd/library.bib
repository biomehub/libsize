doi = {10.1096/fj.15-271304},
file = {:home/ubuntu/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Murthy et al. - 2015 - Alternative activation of macrophages and pulmonary fibrosis are modulated by scavenger receptor, macrophage rece.pdf:pdf},
issn = {15306860},
journal = {FASEB Journal},
keywords = {Asbestos,MARCO,Mitochondria},
number = {8},
pages = {3527--3536},
