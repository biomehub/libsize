journal = {PeerJ},
keywords = {accepted 21 march 2018,metagenomics,microbiome,normalization,published 2 april 2018,rna-seq,statistics,submitted 16 november 2017,zero-inflation},
pages = {e4600},
pmid = {29629248},
title = {{GMPR: A robust normalization method for zero-inflated count data with application to microbiome sequencing data}},
