issn = {1073449X},
journal = {American Journal of Respiratory and Critical Care Medicine},
keywords = {Antiinflammatory,Corticosteroids,Immune cells},
number = {5},
pages = {569--581},
pmid = {21680953},
title = {{Ly6Chimonocytes direct alternatively activated profibrotic macrophage regulation of lung fibrosis}},
