isbn = {1044-1549 (Print)},
issn = {10441549},
journal = {American Journal of Respiratory Cell and Molecular Biology},
keywords = {Apoptosis,CYR61/CCN1,Fas,Inflammation,MMP-12},
number = {2},
pages = {210--221},
pmid = {17446527},
title = {{Essential role of MMP-12 in fas-induced lung fibrosis}},
volume = {37},
