keywords = {Cytokines,Fibrosis,Growth factors,Lung,Myofibroblasts},
number = {1},
pages = {88--100},
pmid = {25543535},
publisher = {Elsevier Ltd},
title = {{The cytokines of pulmonary fibrosis: Much learned, much more to learn}},
url = {http://dx.doi.org/10.1016/j.cyto.2014.11.008},
volume = {74},
year = {2015}
}
