journal = {Current Opinion in Pulmonary Medicine},
keywords = {gene expression,human,review,sarcoidosis,transcriptome,transcriptomics},
pages = {1},
pmid = {28590292},
title = {{Transcriptome profiles in sarcoidosis and their potential role in disease prediction}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28590292{\%}5Cnhttp://insights.ovid.com/crossref?an=00063198-900000000-99291{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/28590292{\%}0Ahttp://Insights.ovid.com/crossref?an=00063198-900000000-99291},
