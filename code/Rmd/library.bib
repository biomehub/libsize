volume = {30},
year = {2017}
}
@article{Naikawadi2016,
abstract = {Telomeres are short in type II alveolar epithelial cells (AECs) of patients with idiopathic pulmonary fibrosis (IPF). Whether dysfunctional telomeres contribute directly to development of lung fibrosis remains unknown. The objective of this study was to investigate whether telomere dysfunction in type II AECs, mediated by deletion of the telomere shelterin protein TRF1, leads to pulmonary fibrosis in mice (SPC-Cre TRF1fl/fl mice). Deletion of TRF1 in type II AECs for 2 weeks increased {\$}\backslashgamma{\{}\backslash{\$}{\}}H2AX DNA damage foci, but not histopathologic changes in the lung. Deletion of TRF1 in type II AECs for up to 9 months resulted in short telomeres and lung remodeling characterized by increased numbers of type II AECs, {\$}\backslashalpha{\{}\backslash{\$}{\}}-smooth muscle actin+ mesenchymal cells, collagen deposition, and accumulation of senescence-associated {\$}\backslashbeta{\{}\backslash{\$}{\}}-galactosidase+ lung epithelial cells. Deletion of TRF1 in collagen-expressing cells caused pulmonary edema, but not fibrosis. These results demonstrate that prolonged telomere dysfunction in type II AECs, but not collagen-expressing cells, leads to age-dependent lung remodeling and fibrosis. We conclude that telomere dysfunction in type II AECs is sufficient to cause lung fibrosis, and may be a dominant molecular defect causing IPF. SPC-Cre TRF1fl/fl mice will be useful for assessing cellular and molecular mechanisms of lung fibrosis mediated by telomere dysfunction.},
author = {Naikawadi, Ram P and Disayabutr, Supparerk and Mallavia, Benat and Donne, Matthew L and Green, Gary and La, Janet L and Rock, Jason R and Looney, Mark R and Wolters, Paul J},
doi = {10.1172/jci.insight.86704},
file = {:home/ubuntu/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Naikawadi et al. - 2016 - Telomere dysfunction in alveolar epithelial cells causes lung remodeling and fibrosis.pdf:pdf},
issn = {2379-3708},
journal = {JCI Insight},
number = {14},
pages = {2--12},
pmid = {27699234},
title = {{Telomere dysfunction in alveolar epithelial cells causes lung remodeling and fibrosis}},
url = {https://insight.jci.org/articles/view/86704},
volume = {1},
year = {2016}
}
