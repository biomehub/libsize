doi = {10.7717/peerj.1621},
file = {:home/ubuntu/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Thompson, Tan, Greene - 2016 - Cross-platform normalization of microarray and RNA-seq data for machine learning applications.pdf:pdf},
