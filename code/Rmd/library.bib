file = {:home/ubuntu/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Shichino et al. - 2015 - Reduced supply of monocyte-derived macrophages leads to a transition from nodular to diffuse lesions and tissue.pdf:pdf},
isbn = {1525-2191 (Electronic)$\backslash$r0002-9440 (Linking)},
