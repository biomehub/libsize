arxivId = {15334406},
author = {Li, Kang and Ching, Daniel and Luk, Fu Sang and Raffai, Robert L},
doi = {10.1161/CIRCRESAHA.117.305844},
eprint = {15334406},
isbn = {1524-4571; 0009-7330},
issn = {15244571},
journal = {Circulation Research},
keywords = {apolipoproteins E,atherosclerosis,macrophages,microRNAs},
number = {1},
pages = {e1----e11},
