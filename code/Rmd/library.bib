file = {:home/ubuntu/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Chai et al. - 2010 - Anti-Aspergillus human host defence relies on type 1 T helper (Th1), rather than type 17 T helper (Th17), cellular.pdf:pdf},
isbn = {1365-2567 (Electronic){\$}\backslashbackslash{\{}\backslash{\$}{\}}r0019-2805 (Linking)},
issn = {00192805},
journal = {Immunology},
keywords = {Aspergillus,Interferon-??,Interleukin-17,Kynurenine,Macrophages},
number = {1},
pages = {46--54},
pmid = {20002791},
title = {{Anti-Aspergillus human host defence relies on type 1 T helper (Th1), rather than type 17 T helper (Th17), cellular immunity}},
volume = {130},
year = {2010}
}
