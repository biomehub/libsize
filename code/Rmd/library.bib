keywords = {Alveolitis,Bronchoalveolar lavage,Cxcr3 ligands,Sarcoidosis,Toll-like receptors},
number = {5},
pages = {749--757},
pmid = {27390897},
title = {{Functional toll-like receptor 9 expression and CXCR3 ligand release in pulmonary sarcoidosis}},
