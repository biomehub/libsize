file = {:home/ubuntu/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Klaus - 2016 - An end to end workflow for differential gene expression using Affymetrix microarrays.pdf:pdf},
issn = {2046-1402},
journal = {F1000Research},
keywords = {gene expression,microarray},
pages = {1384},
title = {{An end to end workflow for differential gene expression using Affymetrix microarrays}},
