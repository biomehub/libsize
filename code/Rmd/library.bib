journal = {Seminars in Immunopathology},
keywords = {Aspergillus,Candida,Macrophages,NK cells,Neutrophils,Pattern recognition receptors},
number = {2},
pages = {107--116},
pmid = {25527294},
title = {{Antifungal innate immunity: recognition and inflammatory networks}},
volume = {37},
year = {2015}
}
