file = {:home/ubuntu/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Ramachandran et al. - 2012 - Differential Ly-6C expression identifies the recruited macrophage phenotype, which orchestrates the regress.pdf:pdf},
isbn = {1474-1741 (Electronic) 1474-1733 (Linking)},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
number = {46},
pages = {E3186--E3195},
pmid = {23100531},
title = {{Differential Ly-6C expression identifies the recruited macrophage phenotype, which orchestrates the regression of murine liver fibrosis}},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.1119964109},
volume = {109},
