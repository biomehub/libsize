journal = {The Journal of Immunology},
keywords = {Adaptor Proteins, Signal Transducing,Adaptor Proteins, Signal Transducing: deficiency,Adaptor Proteins, Signal Transducing: genetics,Adaptor Proteins, Signal Transducing: physiology,Animals,Aspergillus fumigatus,Aspergillus fumigatus: immunology,Cells, Cultured,Immunity, Innate,MAP Kinase Signaling System,MAP Kinase Signaling System: immunology,Macrophages, Alveolar,Macrophages, Alveolar: enzymology,Macrophages, Alveolar: immunology,Mice,Mice, Inbred C57BL,Mice, Knockout,Mitogen-Activated Protein Kinases,Mitogen-Activated Protein Kinases: physiology,Myeloid Differentiation Factor 88,Toll-Like Receptor 2,Toll-Like Receptor 2: deficiency,Toll-Like Receptor 2: genetics,Toll-Like Receptor 2: physiology,Toll-Like Receptor 4,Toll-Like Receptor 4: deficiency,Toll-Like Receptor 4: genetics,Toll-Like Receptor 4: physiology},
