journal = {Biomarkers in medicine},
keywords = {Blood Proteins,Blood Proteins: metabolism,Disease Susceptibility,Genomics,Humans,Individualized Medicine,Matrix Metalloproteinase 7,Matrix Metalloproteinase 7: metabolism,Polymorphism,Pulmonary Fibrosis,Pulmonary Fibrosis: diagnosis,Pulmonary Fibrosis: therapy,Single Nucleotide,Tomography Scanners,Tumor Suppressor Proteins,Tumor Suppressor Proteins: genetics,X-Ray Computed},
number = {4},
pages = {529--540},
pmid = {22917154},
title = {{Personalized medicine: applying 'omics' to lung fibrosis.}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84865501155{\&}partnerID=tZOtx3y1},
