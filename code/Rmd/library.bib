title = {{Apolipoprotein e Enhances MicroRNA-146a in Monocytes and Macrophages to Suppress Nuclear Factor-{\$}\kappa{\$}B-Driven Inflammation and Atherosclerosis}},
volume = {117},
year = {2015}
