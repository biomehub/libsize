eprint = {arXiv:1011.1669v3},
file = {:home/ubuntu/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Appleby et al. - 2013 - Sources of heterogeneity in human monocyte subsets.pdf:pdf},
isbn = {1879-0542 (Electronic){\$}\backslashbackslash{\{}\backslash{\$}{\}}r0165-2478 (Linking)},
issn = {01652478},
journal = {Immunology Letters},
keywords = {CD14,CD16,Human,Monocyte,Phenotype},
number = {1},
pages = {32--41},
pmid = {23557598},
