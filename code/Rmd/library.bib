file = {:home/ubuntu/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Sela-Passwell et al. - 2012 - Antibodies targeting the catalytic zinc complex of activated matrix metalloproteinases show therapeutic po.pdf:pdf},
isbn = {9780470508169},
issn = {10788956},
journal = {Nature Medicine},
number = {1},
pages = {143--147},
pmid = {22198278},
publisher = {Nature Publishing Group},
title = {{Antibodies targeting the catalytic zinc complex of activated matrix metalloproteinases show therapeutic potential}},
url = {http://dx.doi.org/10.1038/nm.2582},
volume = {18},
year = {2012}
