isbn = {1550-6606 (Electronic)$\backslash$r0022-1767 (Linking)},
issn = {0022-1767},
journal = {The Journal of Immunology},
number = {7},
pages = {4780--4790},
pmid = {18802081},
title = {{IL-33 Induces Antigen-Specific IL-5+ T Cells and Promotes Allergic-Induced Airway Inflammation Independent of IL-4}},
