file = {:home/ubuntu/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Molyneaux et al. - 2017 - Host-microbial interactions in idiopathic pulmonary fibrosis.pdf:pdf},
isbn = {1535-4970 (Electronic)
1073-449X (Linking)},
issn = {15354970},
journal = {American Journal of Respiratory and Critical Care Medicine},
keywords = {Acute lung injury,Expression,Idiopathic pulmonary fibrosis,Microbiome,Usual interstitial pneumonia},
