pages = {139--146},
publisher = {Elsevier B.V},
title = {{Augmenting the cytotoxicity of oleic acid-protein complexes: Potential of target-specific antibodies}},
url = {http://dx.doi.org/10.1016/j.biochi.2017.03.013},
volume = {137},
year = {2017}
