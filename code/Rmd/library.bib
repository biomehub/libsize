pmid = {24398220},
title = {{Monocyte and macrophage differentiation: circulation inflammatory monocyte as biomarker for inflammatory diseases}},
url = {http://biomarkerres.biomedcentral.com/articles/10.1186/2050-7771-2-1},
volume = {2},
year = {2014}
