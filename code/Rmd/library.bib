keywords = {BMI = body mass index,LSG = Laparoscopic Sleeve Gastrectomy,Log2FC = Log2 fold change,UCLA = University of California Los Angeles,VIP = Variable importance in projection,YFAS = Yale Food Addiction Scale,appetite,bariatric surgery,food addiction,gut microbiome,hedonic eating,sPLS-DA = Partial Least Square-Discriminant Analys,sleeve gastrectomy},
